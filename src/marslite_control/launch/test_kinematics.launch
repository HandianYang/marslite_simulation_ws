<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- Set the ROS console output format to the current time (H:M:S) -->
    <env name="ROSCONSOLE_FORMAT" value="[${severity}] [${node}]: ${message}"/>

    <!-- Launch the MPC node -->
    <node name="test_kinematics" pkg="marslite_control" type="test_kinematics" output="screen">
    </node>
</launch>